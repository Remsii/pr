#include <stdio.h>

int findGCD(int a, int b) {
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

int findLCM(int arr[], int n) {
    int lcm = arr[0];

    for (int i = 1; i < n; i++) {
        int gcd = findGCD(lcm, arr[i]);
        lcm = (lcm * arr[i]) / gcd;
    }

    return lcm;
}

int main() {
    int n;
    printf("Введіть кількість чисел: ");
    scanf("%d", &n);

    int arr[20];
    printf("Введіть %d натуральних чисел, розділених пробілом: ", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int lcm = findLCM(arr, n);
    printf("Найменше спільне кратне: %d\n", lcm);

    return 0;
}
